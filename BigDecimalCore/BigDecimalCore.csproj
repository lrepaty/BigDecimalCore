<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Lubos Repaty</Authors>
    <Description>An arbitrary precision decimal (base 10) floating point number type using a mantissa and exponent. Supports logarithms, Exp, Pi, trigonometric functions, etc.</Description>
    <PackageTags>biginteger, bigdecimal</PackageTags>
    <AssemblyVersion>1.2.0</AssemblyVersion>
    <FileVersion>1.2.0</FileVersion>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Version>1.2.0</Version>
    <PackageProjectUrl>https://github.com/lrepaty/BigDecimalCore</PackageProjectUrl>
    <Copyright>Copyright Â© Lubos Repaty 2023-2023</Copyright>
    <Title></Title>
    <Company>Lubos Repaty</Company>
    <Product>UciEngine  (Core)</Product>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\ChessDotNetCore\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
